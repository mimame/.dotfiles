{
  "agent": {
    "default_model": {
      "model": "claude-sonnet-4",
      "provider": "zed.dev"
    },
    "model_parameters": [],
    "single_file_review": true
  },
  "auto_install_extensions": {
    "css": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "dracula": true,
    "fish": true,
    "git-firefly": true,
    "html": true,
    "janet": true,
    "json": true,
    "just": true,
    "kdl": true,
    "log": true,
    "lua": true,
    "make": true,
    "material-icon-theme": true,
    "nix": true,
    "ruby": true,
    "ruff": true,
    "sql": true,
    "ssh-config": true,
    "terraform": true,
    "toml": true,
    "typst": true,
    "xml": true,
    "zig": true
  },
  "auto_update": true,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_features": {
    "calt": false
  },
  "buffer_font_size": 16,
  "collaboration_panel": {
    "dock": "right"
  },
  "current_line_highlight": "all",
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "format_on_save": "on",
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "git_panel": {
    "dock": "right"
  },
  "icon_theme": {
    "dark": "Material Icon Theme",
    "light": "Material Icon Theme",
    "mode": "system"
  },
  "inlay_hints": {
    "edit_debounce_ms": 700,
    "enabled": true,
    "scroll_debounce_ms": 50,
    "show_background": false,
    "show_other_hints": true,
    "show_parameter_hints": true,
    "show_type_hints": true,
    "toggle_on_modifiers_press": null
  },
  "load_direnv": "direct",
  "lsp": {
    "taplo": {
      "settings": {
        "array_auto_collapse": false
      }
    },
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true
        }
      }
    }
  },
  "outline_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "relative_line_numbers": true,
  "restore_on_startup": "last_session",
  "selection_highlight": true,
  "tabs": {
    "activate_on_close": "history",
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "show_close_button": "hover"
  },
  "telemetry": {
    "metrics": false
  },
  "terminal": {
    "blinking": "off",
    "copy_on_select": true,
    "font_family": "JetBrainsMono Nerd Font Mono"
  },
  "theme": {
    "dark": "Dracula",
    "light": "Dracula Light (Alucard",
    "mode": "system"
  },
  "ui_font_size": 16,
  "vim_mode": true
}
