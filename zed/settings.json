{
  "agent": {
    "default_model": {
      "model": "claude-3-5-sonnet",
      "provider": "zed.dev"
    },
    "favorite_models": [],
    "inline_assistant_model": {
      "model": "claude-sonnet-4.5",
      "provider": "copilot_chat"
    },
    "model_parameters": []
  },
  "agent_servers": {
    "github-copilot": {
      "type": "registry"
    }
  },
  "auto_install_extensions": {
    "CSS": true,
    "comment": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "dracula": true,
    "elixir": true,
    "fish": true,
    "git-firefly": true,
    "github-copilot": true,
    "html": true,
    "janet": true,
    "json5": true,
    "just": true,
    "kdl": true,
    "log": true,
    "lua": true,
    "make": true,
    "material-icon-theme": true,
    "nix": true,
    "proto": true,
    "ruby": true,
    "sql": true,
    "ssh-config": true,
    "terraform": true,
    "toml": true,
    "typst": true,
    "xml": true,
    "zig": true
  },
  "auto_update": true,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "Maple Mono NL NF",
  "buffer_font_features": {
    "calt": false
  },
  "buffer_font_size": 16,
  "collaboration_panel": {
    "dock": "right"
  },
  "current_line_highlight": "all",
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "format_on_save": "on",
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "git_panel": {
    "dock": "right"
  },
  "icon_theme": {
    "dark": "Material Icon Theme",
    "light": "Material Icon Theme",
    "mode": "system"
  },
  "indent_guides": {
    "coloring": "indent_aware",
    "enabled": true
  },
  "inlay_hints": {
    "edit_debounce_ms": 700,
    "enabled": true,
    "scroll_debounce_ms": 50,
    "show_background": false,
    "show_other_hints": true,
    "show_parameter_hints": true,
    "show_type_hints": true,
    "toggle_on_modifiers_press": null
  },
  "languages": {
    "CSS": {
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "--stdin-file-path",
            "{buffer_path}"
          ],
          "command": "biome"
        }
      }
    },
    "Elixir": {
      "language_servers": [
        "elixir-ls"
      ]
    },
    "Go": {
      "formatter": {
        "external": {
          "arguments": [
            "-"
          ],
          "command": "goimports"
        }
      },
      "language_servers": [
        "gopls"
      ]
    },
    "HTML": {
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "--stdin-file-path",
            "{buffer_path}"
          ],
          "command": "biome"
        }
      }
    },
    "JSON": {
      "formatter": {
        "external": {
          "arguments": [
            "-"
          ],
          "command": "jsonfmt"
        }
      }
    },
    "JavaScript": {
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "--stdin-file-path",
            "{buffer_path}"
          ],
          "command": "biome"
        }
      },
      "language_servers": [
        "vtsls",
        "!typescript-language-server"
      ]
    },
    "Lua": {
      "formatter": {
        "external": {
          "arguments": [
            "-"
          ],
          "command": "stylua"
        }
      }
    },
    "Markdown": {
      "soft_wrap": "editor_width"
    },
    "Nix": {
      "formatter": {
        "external": {
          "arguments": [
            "-"
          ],
          "command": "nixfmt"
        }
      },
      "language_servers": [
        "nixd",
        "!nil"
      ]
    },
    "Python": {
      "formatter": {
        "language_server": {
          "name": "ruff"
        }
      },
      "language_servers": [
        "ruff",
        "pyright"
      ]
    },
    "Ruby": {
      "language_servers": [
        "ruby-lsp"
      ]
    },
    "Rust": {
      "format_on_save": "on",
      "language_servers": [
        "rust-analyzer"
      ]
    },
    "TSX": {
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "--stdin-file-path",
            "{buffer_path}"
          ],
          "command": "biome"
        }
      },
      "language_servers": [
        "vtsls",
        "!typescript-language-server"
      ]
    },
    "TypeScript": {
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "--stdin-file-path",
            "{buffer_path}"
          ],
          "command": "biome"
        }
      },
      "language_servers": [
        "vtsls",
        "!typescript-language-server"
      ]
    },
    "Typst": {
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "-"
          ],
          "command": "typstyle"
        }
      },
      "language_servers": [
        "tinymist"
      ]
    },
    "YAML": {
      "formatter": {
        "external": {
          "arguments": [
            "-"
          ],
          "command": "yamlfmt"
        }
      }
    },
    "Zig": {
      "language_servers": [
        "zls"
      ]
    }
  },
  "lsp": {
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true
        }
      }
    }
  },
  "outline_panel": {
    "dock": "right"
  },
  "preferred_line_length": 120,
  "project_panel": {
    "dock": "right"
  },
  "relative_line_numbers": "enabled",
  "restore_on_startup": "last_session",
  "scroll_beyond_last_line": "one_page",
  "selection_highlight": true,
  "show_whitespaces": "selection",
  "soft_wrap": "preferred_line_length",
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "activate_on_close": "history",
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "show_close_button": "hover"
  },
  "telemetry": {
    "metrics": false
  },
  "terminal": {
    "blinking": "off",
    "copy_on_select": true,
    "font_family": "Maple Mono NL NF"
  },
  "theme": {
    "dark": "Dracula",
    "light": "Dracula Light (Alucard)",
    "mode": "system"
  },
  "ui_font_size": 16,
  "vim_mode": true
}
