{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "auto_install_extension": {
    "catppuccin": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "html": true,
    "lua": true,
    "nix": true,
    "ruff": true,
    "json": true,
    "toml": true
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "autoupdate": false,
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_size": 16,
  "collaboration_panel": {
    "dock": "right"
  },
  "format_on_save": "on",
  "icon_theme": {
    "mode": "system",
    "light": "Catppuccin Frappé",
    "dark": "Catppuccin Mocha"
  },
  "load_direnv": "direct",
  "outline_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "relative_line_numbers": true,
  "restore_on_startup": "last_session",
  "telemetry": {
    "metrics": false
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font Mono",
    "blinking": "off"
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Frappé",
    "dark": "Catppuccin Mocha"
  },
  "ui_font_size": 16,
  "vim_mode": true
}
