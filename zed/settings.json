{
    "theme": "Catppuccin Mocha",
    "telemetry": {
        "metrics": false
    },
    "vim_mode": true,
    "ui_font_size": 16,
    "buffer_font_size": 16,
    "buffer_font_family": "JetBrainsMono Nerd Font Mono",
    "format_on_save": "on",
    "terminal": {
        "font_family": "JetBrainsMono Nerd Font Mono",
        "blinking": "off"
    },
    "autosave": {
        "after_delay": {
            "milliseconds": 1000
        }
    },
    "autoupdate": false
}
